{
    "name": "stematic/testing",
    "description": "Testing and coding standards package.",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": "^8.0.2",
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^9.5|^10.0",
        "phploc/phploc": "^7.0"
    },
    "require-dev": {
        "slevomat/coding-standard": "^8.0.0",
        "webimpress/coding-standard": "^1.2.4",
        "laravel-zero/phar-updater": "^1.2",
        "nunomaduro/larastan": "^2.0",
        "sebastian/phpcpd": "^6.0",
        "laravel-zero/framework": "^v10.0.0",
        "spatie/laravel-ignition": "^2.0.0",
        "orchestra/testbench": "^v8.0.0",
        "nunomaduro/collision": "^v6.0.0",
        "nunomaduro/patrol": "^1.1",
        "nunomaduro/termwind": "^1.3",
        "laminas/laminas-text": "2.10.x-dev",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Stematic\\Testing\\": "src/"
        }
    },
    "bin": ["builds/testing"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
